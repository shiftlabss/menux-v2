{
  "name": "menux-backend-node",
  "version": "1.0.0",
  "description": "Menux Backend recreated with Node.js, TypeScript, DDD and Clean Architecture",
  "main": "dist/interfaces/http/server.js",
  "scripts": {
    "dev": "nodemon",
    "dev2": "nodemon --watch 'src/**/*.ts' --exec 'ts-node  --files -r tsconfig-paths/register' src/interfaces/http/server.ts",
    "build": "tsc",
    "start": "node dist/interfaces/http/server.js",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "lint:fix": "eslint 'src/**/*.{ts,js}' --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "test": "jest",
    "test:integration": "jest -c jest.integration.config.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/infrastructure/database/typeorm/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/infrastructure/database/typeorm/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/infrastructure/database/typeorm/data-source.ts",
    "seed:initial": "ts-node -r tsconfig-paths/register src/infrastructure/database/seeds/seed-initial-data.ts",
    "seed:menu": "DEBUG=typeorm:query ts-node -r tsconfig-paths/register src/infrastructure/database/seeds/seed-menu-data.ts",
    "seed:orders": "DEBUG=typeorm:query ts-node -r tsconfig-paths/register src/infrastructure/database/seeds/seed-order-data.ts",
    "worker:analytics": "ts-node -r tsconfig-paths/register src/workers/analyticsWorker.ts",
    "migrate:images": "ts-node -r tsconfig-paths/register src/infrastructure/database/scripts/migrate-images-to-s3.ts"
  },
  "keywords": [],
  "author": "Shift Labs",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "@aws-sdk/client-s3": "^3.500.0",
    "pg": "^8.11.3",
    "pino": "^8.18.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}